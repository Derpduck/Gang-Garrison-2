    file_text_write_string(cfgFile,"cfg_maxPlayers="+string(cfg_maxPlayers));file_text_writeln(cfgFile)
    file_text_write_string(cfgFile,"cfg_medCabs="+string(cfg_medCabs));file_text_writeln(cfgFile)
    file_text_write_string(cfgFile,"cfg_spawnDoors="+string(cfg_spawnDoors));file_text_writeln(cfgFile)
