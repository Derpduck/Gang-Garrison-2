//Generate a new text document that defines a bunch of values
cfg_name=string(get_string("Name",""))
if cfg_name!=""{
    cfg_rup=real(get_integer("Ready Up Enabled",1))
    cfg_timeLimit=real(get_integer("Time Limit",10))
    cfg_respawnTime=real(get_integer("Respawn Time",5))
    cfg_capLimit=real(get_integer("Capture Limit",4))
    cfg_arenaWinLimit=real(get_integer("Arena Win Limit",5))
